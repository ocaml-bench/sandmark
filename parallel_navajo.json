    {
      "name": "orunchrt",
      "command": "orun -o %{output} -- %{paramwrapper} chrt -r 1 %{command}"
    },
    {
      "name": "perfstat",
      "command": "perf stat -o %{output} -- %{paramwrapper} %{command}"
    },
    {
      "name": "pausetimes_multicore",
